    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.6.3",
    "concurrently": "^7.1.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "framer-motion": "^6.3.2",
    "path": "^0.12.7",
    "per-env": "^1.0.2",
    "react-router-dom": "^6.3.0",
    "serve": "^13.0.2"
  },
  "engines": {
    "node": "16.14.2",
    "npm": "8.5.0"
  },
  "root": "client/"
